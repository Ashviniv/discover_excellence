=render "shared/header"
%h3 Edit a Match
=form_for @match do |f|
  =@match.errors.full_messages unless @match.valid?
  %h5 Enter details
  =f.label "venue"
  =f.text_field :venue
  %br
  =f.label "played_on"
  =f.text_field :played_on
  %br
  =f.fields_for :players do |m, i|
    %br
    =m.label "Player#{i}"
    =m.text_field :name
  %br
  =f.submit
